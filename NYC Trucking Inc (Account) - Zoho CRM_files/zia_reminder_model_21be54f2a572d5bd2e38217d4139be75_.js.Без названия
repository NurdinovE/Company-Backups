store.registerModel("zia_reminder",{id:Lyte.attr("string"),reminderTime:Lyte.attr("string"),layoutCount:Lyte.attr("object")}),store.registerAdapter("zia_reminder",{namespace:"crm/di/",headersForRequest:function(){return{"X-CRM-ORG":crmZgid,"X-ZCSRF-TOKEN":csrfParamName+"="+csrfToken}},buildURL:function(e,r,t,i,n){return n=n.replace("zia_reminder","ziaReminder")},methodForRequest:function(){return"POST"}}),store.registerSerializer("zia_reminder",{normalizeResponse:function(e,r,t){return t.result&&(t[e]={reminderTime:t.ReminderTime,layoutCount:t.layoutCount}),t},normalize:function(e,r,t){if(t)return t.id=Math.random(),t},payloadKey:function(e){if(e)return e},serialize:function(e,r,t,i,n,o){if(o&&"restore"===o.operation&&i)return i}});
