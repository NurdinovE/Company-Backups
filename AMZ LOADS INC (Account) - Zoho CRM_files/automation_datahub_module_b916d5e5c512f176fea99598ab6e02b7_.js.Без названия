store.registerModel("automation_datahub_module",{id:Lyte.attr("string",{primaryKey:!0}),name:Lyte.attr("string",{default:""}),api_name:Lyte.attr("string",{default:""}),data_hub:Lyte.attr("object",{default:{}}),date_time_format:Lyte.attr("object",{default:{}}),schema_props:Lyte.attr("array",{default:[]})}),store.registerAdapter("automation_datahub_module",{namespace:"crm/v6/settings",buildURL:function(t,a,e,o,u,d,r){return u=u.replace("automation_datahub_module","automation_datahub_modules"),r&&r.type&&"get_fields_associations"===r.type?u=u.append("field_associations"):r&&r.datahubTabId&&""!==r.datahubTabId&&(u=u.append("/"+r.datahubTabId)),u},processRequest:function(){commonUtils.showHideLoadingDiv(!0)},methodForRequest:function(t){return"PATCH"===t?"PUT":t},parseResponse:function(t,a,e,o){return o=this.super(arguments)}}),store.registerSerializer("automation_datahub_module",{serialize:function(t,a){var e={};return e.automation_datahub_modules=[a.automation_datahub_module],e},normalizeResponse:function(t,a,e){return"createRecord"!==a&&"updateRecord"!==a||(e.automation_datahub_modules=e.automation_datahub_modules[0].details),e.automation_datahub_modules||(e.automation_datahub_modules=[]),e},payloadKey:function(){return"automation_datahub_modules"}});
