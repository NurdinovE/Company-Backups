Lyte.Mixin.register("crm-dataprivacy-mixin",{isGDPREnabledForModule:function(e){return Crm.userDetails.isPrivacySettingEnabled&&Crm.userDetails.privacyPrefEnabledModules.contains(e)},getRequiredConsentFieldsColumnNames:function(e){let r=Array.from(["PROCESSINGBASIS"]);if(e){let t=e.length;for(let i=0;i<t;i++){const t=r.indexOf(e[i].column_name);t>-1&&r.splice(t,1)}}return r},updateRecordAfterLocking:function(e,r,t,i){let n=store.peekRecord(t,i);n.$stop_processing=!!e,r.setData({record:n,record_changed:!0}),Lyte.registeredMixins["crm-detailview-common-utils"].refreshProcessLayer(n)}});
