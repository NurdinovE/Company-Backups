Lyte.Component.registerHelper("isEdit",(function(e,n,s){var r=!!moduleRecordMapping[this.component.data.module]&&"team_based"===moduleRecordMapping[this.component.data.module].access_type?Crm.userDetails.permissions["Crm_Implied_Manage_CustomViews_"+this.component.data.module]:Crm.userDetails.permissions.Crm_Implied_Manage_CustomViews;return!("Edit"===n&&e&&-1!=e.indexOf("CONVERTEDVIEWS")||!r||"mLookup"==s)})),Lyte.Component.registerHelper("removeSpecialChars",(function(e){return e.replace(/[^a-zA-Z0-9]+/g,"")})),Lyte.Component.registerHelper("isPermission",(function(e,n){var s=n||this.component.data.module;return moduleRecordMapping[s]&&moduleRecordMapping[s].access_type&&"team_based"===moduleRecordMapping[s].access_type?!!Lyte.Component.registeredHelpers.isTeamModuleAdmin(s)||!!Lyte.Component.registeredHelpers.isTeamModuleManager(s):"kanban"===e?!!Crm.userDetails.permissions.Crm_Implied_Manage_View_Kanban:"chart"===e?!!Crm.userDetails.permissions.Crm_Implied_Manage_View_Chart:"timeline"!==e||!!Crm.userDetails.permissions.Crm_Implied_Manage_Timeline})),Lyte.Component.registerHelper("checkNewExportAndCv",(function(){var e=$L("crm-customview-layer")[0].component.data;return!e.cv_sys_name||!(e.cv_sys_name.includes("SHAREDBYME")||e.cv_sys_name.includes("SHAREDTOME")||e.cv_sys_name.includes("COOWNER")||"My Meetings"===e.cv_name)}));
