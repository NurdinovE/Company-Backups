store.registerModel("business_hours",{id:Lyte.attr("string"),business_hours:Lyte.attr("object"),business_days:Lyte.attr("array"),type:Lyte.attr("string"),week_starts_on:Lyte.attr("string"),same_as_everyday:Lyte.attr("boolean"),details:Lyte.attr("array")}),store.registerAdapter("business_hours",{namespace:"crm/v2/settings/",buildURL:function(e,r,s,t,i,a,n){return void 0!==n&&(i=i.match(RebrandLinkUtil.properties.SANDBOXURL)?i.replace(i.split(RebrandLinkUtil.properties.SANDBOXURL)[1],n.url):i.replace(sandbox.orgInfo.crmSandboxOrParentPortal+"/",""),(i=n.isSandboxDiff&&n.url?window.location.origin+n.url:i.replace(i.split(RebrandLinkUtil.properties.CRMURL)[1],n.url)).match("sandbox/diff")||!n.current||i.match("production/diff")||(i=i.replace(i.split("crm/")[1].split("v2.2/")[0],""))),"updateRecord"===r&&(i=i.replaceAll("/"+t.id,"")),i},methodForRequest:function(e,r,s,t){return"PATCH"===e?"PUT":t&&!0===t.isDelete?"DELETE":e}}),store.registerSerializer("business_hours",{normalizeResponse:function(e,r,s){return"updateRecord"===r||"createRecord"===r?(s.hasOwnProperty("business_hours")||(s.business_hours={}),s&&(s.hasOwnProperty("id")||s.hasOwnProperty("details")&&s.details.hasOwnProperty("id")&&(s.id=s.details.id,s.business_hours.id=s.details.id)),s):("findAll"===r&&(s.hasOwnProperty("business_hours")||(s.business_hours={})),s)},normalize:function(e,r,s){return s&&(s.hasOwnProperty("id")||s.hasOwnProperty("details")&&s.details.hasOwnProperty("id")&&(s.id=s.details.id),s.isBusinessHourCreated=void 0!==s.type),s},serialize:function(e,r){return void 0!==r&&r.hasOwnProperty("type")&&("24*7"===r.type?(delete r.details,delete r.business_days,delete r.same_as_everyday):"24*5"===r.type&&(delete r.details,delete r.same_as_everyday)),r},payloadKey:function(){return"business_hours"}});
