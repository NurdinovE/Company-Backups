var gS="object"!=typeof gS?{}:gS;gS.viewPop3=function(e){-1===jQuery.inArray("mail",usernameNotNeeded)&&commonUtils.checkUsernameNeeded((function(){"setup"==e?Utils.windowLocationReload(!0):gS.viewPopMailConfig()}),"mail")},gS.viewPopMailConfig=function(){commonUtils.showHideLoadingDiv(!0);var e=new crmRequestPool,i=Crm.getCrmBasePath()+"/CustomerOnBoarding.do?tab=emailInteg&action=mailAccConfig";e.initiate({type:"POST",action:i,success:function(e){var i=$("#cobdiv"),a=$("#alertDiv");commonUtils.showHideLoadingDiv(),$("#helpdesk").hasClass("cobclicked")?(i.show(),$("#cobdiv .zcrm-content").html(e),a.remove(),i.append(a)):$("#result").html(e)}}),Crm.trackSpotLightAction("Customer OnBoarding - Add Pop Account")},gS.viewimapConfig=function(e,i,a){if($("#sub_emailSettings").hasClass("sel")){var o=I18n.getMsg("cob.alreadyin.setup.mailpage");crmui.showMsgBand("warning",o,5e3)}else{if($("#helpdesk").addClass("cobclicked"),"Pop3"==(e=$("#service").val()))var t={pageTitle:"Zoho Mail ",clickId:"general_emailSettings",tab:"comm",subTab:"emailSettings",calleeObj:"crmSetup",action:"checkConfiguration",conf:"true",service:e,frmPage:"COB",freshConf:a},n={url:Crm.getCrmBasePath()+"/ZMailConfig.do",params:t,title:"crm.label.email.integration",frmPage:"COB"};else t={pageTitle:"crm.label.imap.integration",clickId:"general_emailSettings",tab:"comm",subTab:"emailSettings",calleeObj:"crmSetup",action:"showConfig",conf:"true",service:e,frmPage:"COB",freshConf:a,imapuname:i},n={url:Crm.getCrmBasePath()+"/specific/IMAPAction.do",params:t,title:"crm.label.imap.integration",frmPage:"COB"};crmSetup.loadInnerPageResponse(n,t,"comm","emailSettings")}},gS.hideAnimateCOBalert=function(){$("#helpdesk").hasClass("cobclicked")||renderingUtils.hideFreezeLayer(),crmui.hidePopup("cobdiv_alert")},gS.hideAnimateGLS=function(){$("#helpdesk").hasClass("cobclicked")&&$("body").removeClass("oH"),crmui.hidePopup("cobdiv_alert")};