Lyte.Component.register("crm-attachments",{_template:'<template tag-name="crm-attachments"> <template is="for" items="{{actions}}" item="item" index="index"> <lyte-menu-item data-value="{{item[\'data-cid\']}}" onclick="{{action(\'attachmentClickAction\',item[\'data-cid\'])}}" data-zcqa="{{if(fromBlueprint,expHandlers(\'blueprint_\',\'+\',item.zcqa),item.zcqa)}}"> <lyte-menu-label><span class="{{getAttachmentIcons(item[\'data-cid\'])}}"></span>{{item.label}}</lyte-menu-label> </lyte-menu-item> </template> <input type="hidden" id="isAttachLyte" value="true"> </template>',_dynamicNodes:[{type:"attr",position:[1]},{type:"for",position:[1],dynamicNodes:[{type:"attr",position:[1]},{type:"attr",position:[1,1,0]},{type:"text",position:[1,1,1]},{type:"componentDynamic",position:[1,1]},{type:"componentDynamic",position:[1]}]}],_observedAttributes:["from"],data:function(){return{from:Lyte.attr("string",{default:void 0})}},actions:{attachmentClickAction:function(t){this.executeRelatedListActions(t,this.data.relatedList,this.data.module,this.data.record,event,void 0,this.getData("from"))}},init:function(){this.getData("relatedList").actions&&Crm.userDetails.isRelatedListLyteEnabled?this.setData("actions",this.data.relatedList.actions):this.setData("actions",this.getAttachmentActions("RL_Attachments"))}},{mixins:["crm-detail-view-utils","crm-cloud-picker-mixin","crm-related-list-utils"]});
