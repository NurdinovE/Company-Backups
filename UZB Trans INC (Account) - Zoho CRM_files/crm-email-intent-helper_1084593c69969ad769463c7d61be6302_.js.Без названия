Lyte.Component.registerHelper("getTranslatedIntent",(function(e,r){var s="";switch(e){case"Request":s="crm.zia.CI.system.request";break;case"Query":s="crm.zia.CI.system.query";break;case"Complaint":s="crm.zia.CI.system.complaint";break;case"Purchase":s="crm.zia.CI.system.purchase";break;case"Others":s="Others";break;default:return r?I18n.getMsg("crm.zia.CI.name"):e}return r?I18n.getMsg("crm.zia.CI.system.name"):I18n.getMsg(s)}));
